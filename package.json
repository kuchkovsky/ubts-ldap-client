{
  "name": "ubts-ldap-client",
  "version": "1.0.5",
  "description": "UBTS LDAP Client",
  "main": "main.js",
  "scripts": {
    "bundle": "webpack --mode development",
    "serve": "electron . --dev",
    "start": "npm-run-all --parallel bundle serve",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Yaroslav Kuchkovsky <y.kuchkovsky@gmail.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuchkovsky/ubts-ldap-client.git"
  },
  "homepage": "https://github.com/kuchkovsky/ubts-ldap-client",
  "build": {
    "productName": "UBTS LDAP Client",
    "appId": "ua.org.ubts.ldapclient",
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": [
        "snap",
        "deb"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "extraResources": [
      ".env",
      "ca.crt"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.6.0",
    "electron-devtools-installer": "^3.0.0",
    "file-loader": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "cyrillic-to-translit-js": "^3.1.0",
    "dotenv-webpack": "^1.8.0",
    "electron-reload": "^1.5.0",
    "immutable": "^4.0.0-rc.12",
    "ldapjs": "^1.0.2",
    "password-generator": "^2.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.3.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "with-immutable-props-to-js": "^2.0.1"
  }
}
